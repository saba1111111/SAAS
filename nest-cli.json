{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "account": {
      "type": "library",
      "root": "libs/account",
      "entryFile": "index",
      "sourceRoot": "libs/account/src",
      "compilerOptions": {
        "tsConfigPath": "libs/account/tsconfig.lib.json"
      }
    },
    "plan": {
      "type": "library",
      "root": "libs/plan",
      "entryFile": "index",
      "sourceRoot": "libs/plan/src",
      "compilerOptions": {
        "tsConfigPath": "libs/plan/tsconfig.lib.json"
      }
    },
    "redis": {
      "type": "library",
      "root": "libs/redis",
      "entryFile": "index",
      "sourceRoot": "libs/redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis/tsconfig.lib.json"
      }
    },
    "subscription": {
      "type": "library",
      "root": "libs/subscription",
      "entryFile": "index",
      "sourceRoot": "libs/subscription/src",
      "compilerOptions": {
        "tsConfigPath": "libs/subscription/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "compression": {
      "type": "library",
      "root": "libs/compression",
      "entryFile": "index",
      "sourceRoot": "libs/compression/src",
      "compilerOptions": {
        "tsConfigPath": "libs/compression/tsconfig.lib.json"
      }
    }
  }
}
